{
    "builds": [
    {
        "src": "mystudybud/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" }
    }
    ],
    "routes": [
    {
        "src": "/static/(.*)",
        "dest": "/static/$1"
    },
    {
        "src": "/(.*)",
        "dest": "mystudybud/wsgi.py"
    }
    ],
    "scripts": {
    "postinstall": "python manage.py collectstatic --noinput"
    }
}